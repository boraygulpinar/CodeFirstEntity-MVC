@using Skill_CodeFirstEntity.Models.Classes
@model List<Skill>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>SKILL</th>
        <th>VALUE</th>
        <th>DELETE</th>
        <th>UPDATE</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.SkillID</td>
            <td>@item.SkillDescription</td>
            <td>@item.SkillValue</td>
            <td><a href="/Admin/SkillDelete/@item.SkillID" class="btn btn-danger">Delete</a></td>
            <td><a href="/Admin/SkillGet/@item.SkillID" class=" btn btn-success">Update "Calismiyor"</a></td>
        </tr>
    }
</table>